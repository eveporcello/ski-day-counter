{
  "name": "ski-day-counter",
  "version": "0.0.5",
  "description": "A sample React application for use in lynda.com's React Essential Training with Eve Porcello",
  "main": "index.js",
  "scripts": {
    "lint": "./node_modules/.bin/eslint .",
    "analyze": "./node_modules/.bin/webpack --json | webpack-bundle-size-analyzer",
    "build": "./node_modules/.bin/webpack --progress",
    "prestart": "npm run build",
    "start": "node ./server --port 3333 --delay 500",
    "dev": "open http://localhost:3000 & ./node_modules/.bin/webpack-dev-server"
  },
  "keywords": [
    "React",
    "state",
    "setState",
    "explicitly",
    "passing",
    "props"
  ],
  "author": "Alex Banks <alex@moonhighway.com> (http://www.moonhighway.com)",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer-loader": "^3.2.0",
    "babel-cli": "^6.11.4",
    "babel-loader": "^6.2.1",
    "babel-plugin-rewire": "^1.0.0-rc-5",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "cors": "^2.8.1",
    "css-loader": "^0.23.1",
    "eslint": "^3.2.2",
    "eslint-plugin-react": "^6.0.0",
    "json-loader": "^0.5.4",
    "node-sass": "^3.4.2",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.12",
    "webpack-bundle-size-analyzer": "^2.2.0",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "express": "^4.14.0",
    "isomorphic-fetch": "^2.2.1",
    "ramda": "^0.22.1",
    "react": "^15.3.2",
    "react-dom": "^0.14.6",
    "react-icons": "^2.2.1",
    "react-router": "^2.8.1"
  }
}
